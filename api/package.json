{
  "name": "gql-boilerplate",
  "version": "1.0.0",
  "description": "ApolloServer GQL Boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "node -r esm index.js",
    "dev": "nodemon -r esm index.js",
    "migrate:make": "knex --knexfile knexfile.js migrate:make",
    "migrate:run": "knex --knexfile knexfile.js migrate:latest",
    "migrate:rollback": "knex --knexfile knexfile.js migrate:rollback",
    "migrate:status": "knex --knexfile knexfile.js migrate:status",
    "seed:make": "knex --knexfile knexfile.js seed:make",
    "seed": "knex --knexfile knexfile.js seed:run",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "author": "brunojkurt",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.21.0",
    "bcryptjs": "^2.4.3",
    "esm": "^3.2.25",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.18",
    "pg": "^8.5.1",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "apollo-server-testing": "^2.25.1",
    "babel-jest": "^27.0.2",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "standard": "^16.0.3"
  }
}
